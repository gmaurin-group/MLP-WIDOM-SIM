&GLOBAL
  PROJECT widom_qmof-a9be673_mepoml-geo_opt_odac_H2O-PBE_e_min-opt_0005
  PRINT_LEVEL LOW
  RUN_TYPE ENERGY
&END GLOBAL

&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    CHARGE 0
    MULTIPLICITY 1
    UKS .FALSE.
    &MGRID
      NGRIDS 4
      CUTOFF 450
      REL_CUTOFF 50
    &END
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-12
      EXTRAPOLATION ASPC
    &END
   &SCF
     SCF_GUESS RESTART
     EPS_SCF 1.0E-6
     MAX_SCF 500
     &OUTER_SCF
       EPS_SCF 1.0E-6
       MAX_SCF 500
     &END
     &OT
       MINIMIZER DIIS
       PRECONDITIONER FULL_KINETIC
       SAFE_DIIS ON
       ENERGY_GAP 0.001
     &END
   &END
    &XC
      &XC_FUNCTIONAL PBE
      &END
      &vdW_POTENTIAL
        POTENTIAL_TYPE PAIR_POTENTIAL
        &PAIR_POTENTIAL
          TYPE DFTD3
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL PBE
          CALCULATE_C9_TERM .TRUE.
          R_CUTOFF 12.
          EPS_CN 0.01
          LONG_RANGE_CORRECTION .TRUE.
          SHORT_RANGE_CORRECTION .TRUE.
        &END
      &END
    &END
    &POISSON
      PERIODIC XYZ
    &END
  &END
  &SUBSYS
    &CELL
      ABC 6.690000 11.094000 13.168000
      ALPHA_BETA_GAMMA 90.00 90.00 101.40
    &END
    &TOPOLOGY
      COORD_FILE_FORMAT CIF
      COORD_FILE_NAME widom_qmof-a9be673_mepoml-geo_opt_odac_H2O-PBE_e_min-opt_0005.cif
      &GENERATE
        REORDER .FALSE.
      &END
    &END
    &KIND Al
      BASIS_SET DZVP-MOLOPT-SR-GTH-q3
      POTENTIAL GTH-PBE-q3
    &END KIND
    &KIND C
      BASIS_SET DZVP-MOLOPT-SR-GTH-q4
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH-q1
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-MOLOPT-SR-GTH-q6
      POTENTIAL GTH-PBE-q6
    &END KIND
  &END
  STRESS_TENSOR ANALYTICAL
&END
